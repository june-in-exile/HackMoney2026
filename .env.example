# Network Selection
NEXT_PUBLIC_NETWORK=testnet

# Verification Keys (hex format)
# Unshield VK (360 bytes) from circuits/build/unshield_vk_bytes.hex
TESTNET_UNSHIELD_VK=<hex from circuits/build/unshield_vk_bytes.hex>
MAINNET_UNSHIELD_VK=<hex from circuits/build/unshield_vk_bytes.hex>

# Transfer VK (424 bytes) from circuits/build/transfer_vk_bytes.hex
TESTNET_TRANSFER_VK=<hex from circuits/build/transfer_vk_bytes.hex>
MAINNET_TRANSFER_VK=<hex from circuits/build/transfer_vk_bytes.hex>

# Swap VK (456 bytes) from circuits/build/swap_vk_bytes.hex
TESTNET_SWAP_VK=<hex from circuits/build/swap_vk_bytes.hex>
MAINNET_SWAP_VK=<hex from circuits/build/swap_vk_bytes.hex>

# Testnet Contract Addresses
NEXT_PUBLIC_TESTNET_PACKAGE_ID=<package id from deploy_package.sh>
NEXT_PUBLIC_TESTNET_USDC_TYPE=0xa1ec7fc00a6f40db9693ad1415d0c193ad3906494428cf252621037bd7117e29::usdc::USDC
NEXT_PUBLIC_TESTNET_SUI_POOL_ID=<pool id from create_pool.sh>
NEXT_PUBLIC_TESTNET_USDC_POOL_ID=<pool id from create_pool.sh>

# Mainnet Contract Addresses
NEXT_PUBLIC_MAINNET_PACKAGE_ID=<package id from deploy_package.sh>
NEXT_PUBLIC_MAINNET_USDC_TYPE=0xdba34672e30cb065b1f93e3ab55318768fd6fef66c15942c9f7cb846e2f900e7
NEXT_PUBLIC_MAINNET_SUI_POOL_ID=<pool id from create_pool.sh>
NEXT_PUBLIC_MAINNET_USDC_POOL_ID=<pool id from create_pool.sh>

# DeepBook (mainnet only)
